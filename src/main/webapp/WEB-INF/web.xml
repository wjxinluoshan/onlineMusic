<!DOCTYPE web-app PUBLIC
  "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN"
  "http://java.sun.com/dtd/web-app_2_3.dtd" >

<web-app xmlns="http://xmlns.jcp.org/xml/ns/javaee"
  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee
                      http://xmlns.jcp.org/xml/ns/javaee/web-app_4_0.xsd" version="4.0">
  <display-name>Archetype Created Web Application</display-name>

  <servlet>
    <servlet-name>staticServlet</servlet-name>
    <servlet-class>com.ajobs.servlet.StaticServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>staticServlet</servlet-name>
    <url-pattern>/html/.html</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>staticServlet</servlet-name>
    <url-pattern>/js/*</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>staticServlet</servlet-name>
    <url-pattern>/css/*</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>staticServlet</servlet-name>
    <url-pattern>/layui/*</url-pattern>
  </servlet-mapping>


  <servlet>
    <servlet-name>servlet</servlet-name>
    <servlet-class>com.ajobs.servlet.Servlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>servlet</servlet-name>
    <url-pattern>/logic/*</url-pattern>
  </servlet-mapping>



</web-app>
